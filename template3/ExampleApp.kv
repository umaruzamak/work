#:kivy 1.10
AnchorLayout:
    anchor_x: 'center'
    anchor_y: 'center'
    Button:
        height: 40
        width: 100
        size_hint: (None, None)
        text: 'Click Me'
        on_press: app.process_button_click()

<PopupBox>:
    pop_up_text: _pop_up_text
    size_hint: .5, .5
    auto_dismiss: True
    title: 'Statusdsadasd'   

    BoxLayout:
        orientation: "vertical"
        AsyncImage:
            source: '.\spinner.gif'
            anim_delay: 0.05
        Label:
            id: _pop_up_text
            text: 'asdasd'

